{
  "name": "merng-demo",
  "version": "1.0.0",
  "description": "Demo App using Mongo DB, Node js, Reatc js and GraphQL with Prisma",
  "main": "src/index.ts",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "deploy": "cd prisma && prisma deploy && prisma generate",
    "generate:graphql": "graphqlgen"
  },
  "author": "Owen Brice",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^5.0.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.0.4",
    "dotenv-cli": "^3.1.0",
    "graphqlgen": "^0.6.0-rc9",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
